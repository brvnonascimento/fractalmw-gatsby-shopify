{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "module": "commonjs",
        "lib": [
            "ESNext",
            "DOM"
        ],
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
    },
    "exclude": [
        "node_modules"
    ]
}